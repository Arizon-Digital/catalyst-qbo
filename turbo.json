{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "signature": true
  },
  "globalDependencies": [
    ".env.local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "env": ["*"],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "core#build": {
      "dependsOn": ["^build"],
      "env": ["*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": ["*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "env": ["*"]
    },
    "typecheck": {
      "dependsOn": [
        "^build"
      ],
      "env": ["*"],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["*"]
    }
  }
}